{
  "name": "",
  "version": "0.1.0",
  "main": "src/index.js",
  "babel": {
    "stage": 1
  },
  "author": {
    "name": "Daniel Whiffing",
  },
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -r $npm_package_config_junto_path",
    "docs": "npm run docs:clean; npm run docs:build; npm run docs:serve;",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --compilers js:babel/register -R dot -r ./test/jsdom.js ./test/**/*Tests.*",
    "build": "if [ $npm_package_config_junto_path == null ]; then cat junto_path_warning; else npm run build:run; fi",
    "watch": "if [ $npm_package_config_junto_path == null ]; then cat junto_path_warning; else npm run watch:run; fi",
    "docs:build": "babel-node docs/build",
    "docs:debug": "babel-node --debug-brk docs/build",
    "docs:serve": "http-server -o -p 8080 docs/build",
    "docs:clean": "rm -r docs/build",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "test:cov": "env NODE_PATH=$NODE_PATH:$PWD/src istanbul cover _mocha -- --compilers js:babel/register -r ./test/jsdom.js -u exports -R dot ./test/**/*Tests.js*",
    "build:run": "npm run clean && npm run build:assets && npm run webpack:base -- --config ./webpack.prod.js",
    "watch:run": "npm run clean && npm run build:assets && npm run webpack:base -- --config ./webpack.dev.js --watch ",
    "build:assets": "mkdir -p $npm_package_config_junto_path/images; cp ./assets/images/* $npm_package_config_junto_path/images",
    "webpack:base": "webpack --progress --color --output-path $npm_package_config_junto_path",
    "webpack:stats": "webpack --config webpack.prod.js --profile --json > webpack-stats.json"
  },
  "dependencies": {
    "bluebird": "^2.10.1",
    "classnames": "^2.1.3",
    "history": "^1.13.0",
    "lodash": "^3.10.1",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^2.0.0",
    "react-router": "1.0.0-rc1",
    "redux": "^3.0.5",
    "redux-actions": "^0.7.0",
    "superagent": "^1.4.0",
    "superagent-bluebird-promise": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.3",
    "babel-core": "^5.6.17",
    "babel-loader": "^5.3.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.2.0",
    "chalk": "^1.1.0",
    "css-loader": "^0.15.2",
    "docchi": "^0.10.5",
    "file-loader": "^0.8.1",
    "highlight.js": "^8.6.0",
    "http-server": "^0.8.0",
    "inflection": "^1.7.1",
    "istanbul": "^0.3.21",
    "js-factories": "^1.1.1",
    "jsdom": "^6.5.1",
    "legit-tests": "^0.6.1",
    "marked": "^0.3.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "mustache": "^2.1.2",
    "nock": "^3.3.2",
    "node-sass": "^3.2.0",
    "phantomjs2-ext": "^0.1.0",
    "react-addons-test-utils": "^0.14.0",
    "react-tools": "^0.13.3",
    "sass-loader": "^1.0.2",
    "sinon": "git+https://github.com/sinonjs/sinon.git",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.5",
    "wallaby-webpack": "0.0.9",
    "webpack": "^1.11.0"
  }
}
